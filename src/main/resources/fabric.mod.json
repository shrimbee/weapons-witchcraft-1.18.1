{
  "schemaVersion": 1,
  "id": "weaponswitchcraft",
  "version": "${version}",

  "name": "Weapons&Witchcraft",
  "description": "A mod witch aims to enhance the minecraft combat experience by adding new weapons and a way to customize them to suit your own play style. It also adds magic spells which have a variety of uses including combat.",
  "authors": [
    "shrimpee"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.shrimpee.weaponswitchcraft.WeaponsWitchcraft"
    ]
  },
  "mixins": [
    "weaponswitchcraft.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
